    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
